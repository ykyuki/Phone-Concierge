{
  "name": "twilio-voice-javascript-sdk-quickstart-node",
  "private": true,
  "version": "1.0.0",
  "description": "Quick Start application template for Twilio Voice JavaScript SDK on Node.js",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "eslint": "./node_modules/.bin/eslint .",
    "test": "./node_modules/.bin/jest",
    "make-vas": "pacmd load-module module-null-sink sink_name=VoiceOutput sink_properties=device.description=VoiceOutput && pacmd load-module module-null-sink sink_name=AssistantOutput sink_properties=device.description=AssistantOutput",
    "remove-vas": "pacmd unload-module module-null-sink"
  },
  "repository": {
    "type": "git",
    "url": "github.com/TwilioDevEd/client-quickstart-node"
  },
  "keywords": [
    "twilio",
    "voip",
    "ip",
    "chat",
    "real",
    "time",
    "diggity"
  ],
  "author": "Twilio Developer Education",
  "license": "MIT",
  "engines": {
    "node": ">=10.x"
  },
  "dependencies": {
    "@twilio/voice-sdk": "^2.0.1",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "google-assistant": "^0.7.0",
    "speaker": "^0.5.4",
    "twilio": "~3.64.0",
    "node-record-lpcm16": "git+https://github.com/endoplasmic/node-record-lpcm16.git"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^27.0.5",
    "prettier": "2.3.1"
  }
}
